{
  "name": "sticky-bookmarks",
  "displayName": "Sticky Bookmarks",
  "description": "Lock bookmarks to code using inline tokens.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [{
      "command": "sticky-bookmarks.toggle",
      "title": "Sticky Bookmarks: Toggle Bookmark"
    },
    {
      "command": "sticky-bookmarks.clearFile",
      "title": "Sticky Bookmarks: Remove Bookmarks in File"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.23.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
