{
  "name":        "sticky-bookmarks",
  "displayName": "Sticky Bookmarks",
  "publisher":   "eridien",
  "description": "Lock bookmarks to code using inline tokens.",
  "version":     "0.0.1",
  "engines": { "vscode": "^1.99.0" },
  "categories": [ "Other" ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sticky-bookmarks.toggle",
        "title": "Sticky Bookmarks: Toggle Bookmark"
      },
      {
        "command": "sticky-bookmarks.prev",
        "title": "Sticky Bookmarks: Previous Bookmark"
      },
      {
        "command": "sticky-bookmarks.next",
        "title": "Sticky Bookmarks: Next Bookmark"
      },
      {
        "command": "sticky-bookmarks.clearFile",
        "title": "Sticky Bookmarks: Remove Bookmarks From File"
      },
      {
        "command": "sticky-bookmarks.clearAllFiles",
        "title": "Sticky Bookmarks: Remove Bookmarks From All Files"
      },
      {
        "command": "sticky-bookmarks.cleanFile",
        "title": "Sticky Bookmarks: Clean File"
      },
      {
        "command": "sticky-bookmarks.cleanAllFiles",
        "title": "Sticky Bookmarks: Clean All Files"
      }
    ],
    "viewsContainers": {
      "activitybar": [{
          "id": "activityBar",
          "title": "Sticky Bookmarks",
          "icon": "images/activity-icon.png"
        }
      ]
    },
    "views": {
      "activityBar": [ {
          "id": "sidebarView",
          "name": "Sticky Bookmarks",
          "icon": "images/activity-icon.png"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.99.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.23.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
